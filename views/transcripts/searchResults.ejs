
<%- include('../partials/head.ejs') %>
<link rel="stylesheet" href="../css/search.css" type="text/css">
<div class="containerWide" id="searchResults">

    <div> Search: <i> <%= search %> </i></div>
    <!-- <div> RESULTS ALL <%= ("%o", results) %> </div> -->
    
    
    <% results.forEach( result => {  %>
        <%- include('./searchResults_vid.ejs', {  
            vidTitle: result.vidTitle,     
            vidPath: result.vidPath,   
            queriedCaptions: result.queriedCaptions   
        })%>
    <% }) %>
    
</div>